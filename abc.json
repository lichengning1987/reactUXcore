{
    "name": "starter-kit",
    "builder": "@ali/builder-alinw",
    "options": {
        "type": "uxcore",
        "template": "https://github.com/nowa-webpack/template-uxcore/archive/master.zip",
        "npm": "npm",
        "vars": {
            "locale": "zh-cn",
            "__LOCAL__": true
        },
        "buildvars": {
            "locale": [ "zh-cn", "en" ]
        },
        "pages": true,
        "libraries": {
            "Uxcore": {
                "output": "uxcore.js",
                "mappings": {
                    "Table": "uxcore-table@~1.5.1",
                    "Form": "uxcore-form@1.4.5",
                    "Button": "uxcore-button@0.4.5"
                }
            }
        }
    }
}
